[Unit]
Description=Drowsiness Detection React Frontend
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/opt/drowsiness-detection/dashboard
ExecStart=/home/pi/.nvm/versions/node/v22.16.0/bin/serve s dist -l 3000
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target